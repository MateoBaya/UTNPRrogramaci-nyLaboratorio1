#include <stdio.h>
#include <stdlib.h>

int sumar(int num1, int num2); //declarar

int main() {
	setbuf(stdout, NULL);
	int num1;
	int num2;
	int res;
	printf("Ingrese número: ");
	scanf("%d",&num1);
	printf("Ingrese otro número: ");
	scanf("%d",&num2);
	sumar(num1,num2);
	printf("%d", res)
	return EXIT_SUCCESS;
}
int sumar(int num1, int num2) {
	int res;
	res = num1 + num2;
	return res;
}
