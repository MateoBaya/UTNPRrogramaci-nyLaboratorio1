#include <stdio.h>
#include <stdlib.h>

int ingNum(); //declarar

int main() {
	setbuf(stdout, NULL);
	int txt;
	txt = ingNum();
	printf("El texto es \n %d", txt); //invocar
	return EXIT_SUCCESS;
}
int ingNum() {
	int numIngresado;
	printf("Ingrese número: ");
	scanf("%d", &&numIngresado); //desarrollo
	return numIngresado;
}
