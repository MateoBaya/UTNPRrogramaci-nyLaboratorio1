#ifndef SRC_CALCULOS_H_
#define SRC_CALCULOS_H_

/*brief Calcula la suma de dos numeros recibidos por parámetro
 * param1 recibe un número entero para sumar/restar
 * param2 recibe un número entero para sumar/restar
 * retorno devuelve la suma/resta de los números recibidos por parámetros
 * */

int sumar(numero1, numero2);
int restar(int numero1, int numero2);

#endif /* SRC_CALCULOS_H_ */
